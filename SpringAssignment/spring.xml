<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">	

	<bean id="pat" class="com.ust.beans.Patient">
		<property name="pid" value="101"></property>
		<property name="name" value="Shamnad"></property>
		<property name="address" value="kollam"></property>
	</bean>
	
	<bean id="med" class="com.ust.beans.Medicine">
		<property name="mid" value="511"></property>
		<property name="mname" value="paracetamol"></property>
		<property name="price" value="200"></property>
	</bean>
	
	<bean id="bill" class="com.ust.beans.Bill">
	<property name="billId" value="401"></property>
	<property name="pid" value="101"></property>
	<property name="mid" value="511"></property>
	</bean>
</beans>